def numeromaior(numero1,numero2,numero3):
  if numero1 > numero2 and numero1 > numero3:
    print(f"o primeiro numero é o maior: {numero1}")
  elif numero2 > numero1 and numero2 > numero3:
    print(f"o segundo numero é o maior:{numero2}")
  elif numero3 > numero1 and numero3 > numero1:
    print(f"o terceiro numero é o maior:{numero3}")
  else:
    print("algo não está certo")


numero1=int(input("digite o primeiro numero"))
numero2=int(input("digite o segundo numero"))
numero3=int(input("digite o terceiro numero"))

numerofinal= numeromaior(numero1,numero2,numero3)
print(numerofinal)
